{
	"name": "df_episodes",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "episodes"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "surrogateKey1",
					"description": "Adding new key episodekey starting from 1 with step 1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> episodes",
				"episodes keyGenerate(output(episode_key as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1"
			]
		}
	}
}